{
	"name": "0x1881/minify",
	"keywords": ["minify", "laravel5", "css", "sass", "scss", "javascript", "js"],
	"description": "A package for automaticaly minifying CSS, SCSS and Javascript for Laravel",
	"license": "MIT",
	"authors": [{
		"name": "Mehmet Can",
		"email": "1881x0@gmail.com"
	}],
	"require": {
		"php": ">=7.4.0",
		"scssphp/scssphp": "^1.0",
		"tedivm/jshrink": "^1.3",
        	"illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0"
	},
	"autoload": {
		"psr-4": {
			"C4N\\Minify\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"C4N\\Minify\\ServiceProvider"
			],
			"aliases": {
				"Minify": "C4N\\Minify\\Facade"
			}
		}
	}
}
